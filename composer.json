{
    "name": "medianova/laravel-sms",
    "description": "SMS Package for laravel",
    "license": "mit",
    "authors": [{
        "name": "Umut Cetinkaya",
        "email": "umut.cetinkaya@medianova.com"
    }],
    "require": {
        "guzzlehttp/guzzle": "^6.5.5|^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "orchestra/testbench": "^v6.18.0"
    },
    "autoload": {
        "psr-4": {
            "Medianova\\LaravelSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Medianova\\LaravelSms\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Medianova\\LaravelSms\\LaravelSmsServiceProvider"
            ],
            "aliases": {
                "Sms": "Medianova\\LaravelSms\\Facades\\Sms"
            }
        }
    }
}
